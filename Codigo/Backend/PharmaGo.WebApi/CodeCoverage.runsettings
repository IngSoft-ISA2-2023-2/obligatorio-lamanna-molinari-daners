							<Exclude>
								<Function>PharmaGo.DataAccess.Migrations\..*</Function>
								<Function>PharmaGo.BusinessLogic.ExportManager</Function>
								<Function>PharmaGo.DataAccess.PharmacyGoDbContext</Function>
